<div class="admin-title">
    <h2 justTheTitle="true">Add New Publication</h2>
</div>

<form action="/add-post" method="post" enctype="multipart/form-data">
    <div class="postIt">

        <label for="title"><b>Title</b></label>
        <input type="" placeholder="Enter the title of the book" class="booktitle"
            style="width: 400px;  border: 1.5px solid black;">


        <label for="content"><b>Add Book</b></label>
        <input type="file" class="book" name="book" style="width: 150px; height: 100px;padding: 10px;">



        <input type="submit" value="Add" class="addPost">
    </div>
</form>